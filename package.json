{
  "name": "vue2-year-picker",
  "version": "1.1.7",
  "description": "這是一個非常簡易而且只有選擇年份的 Vue2 組件，支持西元轉民國年份轉換。",
  "author": "traveltime1221",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/traveltime1221/vue2-year-picker.git" 
  }, 
  "keywords": [
    "vue2",
    "vue-component",
    "year-picker",
    "date-picker"
  ], 
  "bugs": {
    "url": "https://github.com/traveltime1221/vue2-year-picker/issues"
  }, 
  "homepage": "https://github.com/traveltime1221/vue2-year-picker#readme", 
  "type": "module",
  "main": "dist/vue2-year-picker.umd.js",
  "module": "dist/vue2-year-picker.esm.js",
  "exports": {
    ".": {
      "import": "./dist/vue2-year-picker.esm.js",
      "require": "./dist/vue2-year-picker.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^1.0.3",
    "rollup": "^4.30.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue-template-compiler": "^2.7.16"
  }
}
